{
  "manifestVersion": 1,
  "version": "1.1",
  "Id": "VSO-Scrum-Report",
  "name": "Test Scrum Report",
  "description": "Keep track with the team's progress with a scrum report.",
  "publisher": "KriefMikael",
  "public": false,
  "provider": {
    "name": "Mikael Krief"
  },
  "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
  "tags": [
    "scrum",
    "report"
  ],
  "scopes": [
    "vso.work",
    "vso.work_write"
  ],

  "files": [
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "images",
      "addressable": true
    },
    {
      "path": "scrumMeeting.html",
      "addressable": true
    },
    {
      "path": "css",
      "addressable": true
    }
  ],
 

  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "Licence/MIT-1.txt"
    }
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },

  "links": {
    "support": {
      "uri": "https://github.com/mikaelkrief/VSTS-Extension-ScrumReport"
    }
  },

  "icons": {
    "default": "images/squareicon.png"
  },
  "categories": [
    "Plan and track"
  ],
  "contributions": [
    {
      "id": "KriefMikael.ScrumReport",
      "type": "ms.vss-web.hub",
      "description": "Adds a 'Scrum report' hub to the Work hub group.",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "Test Scrum Report",
        "order": 99,
        "uri": "scrumMeeting.html"
      }
    }
  ]


}